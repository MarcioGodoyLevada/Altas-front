{
  "name": "altas-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.23.0",
    "caniuse-lite": "^1.0.30001402",
    "formik": "^2.2.9",
    "jwt-decode": "^3.1.2",
    "polished": "^4.1.3",
    "react": "^18.2.0",
    "react-beforeunload": "^2.5.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.4.5",
    "styled-components": "^5.3.3",
    "styled-icons": "^10.42.0",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@testing-library/dom": "^8.10.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-beforeunload": "^2.1.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.1",
    "@types/styled-components": "^5.1.14",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-loader": "8.1.0",
    "canvas": "^2.8.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^11.1.2",
    "msw": "^0.35.0",
    "prettier": "^2.4.1",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test --watchAll=false --config-jest=jest.config.js",
    "test:watch": "react-scripts test --config-jest=jest.config.js",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
